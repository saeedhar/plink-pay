{
  "name": "plink",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "flowbite-react patch",
    "prisma:generate": "prisma generate --schema server/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema server/prisma/schema.prisma --name init_kyb",
    "prisma:seed": "tsx server/prisma/seed.ts",
    "dev:server": "tsx server/src/index.ts",
    "dev:all": "concurrently \"npm:dev:server\" \"npm:dev\""
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@prisma/client": "^6.16.1",
    "@tailwindcss/vite": "^4.1.8",
    "dotenv": "^17.2.2",
    "fastify": "^5.6.0",
    "flowbite-react": "^0.11.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^6.30.1",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "4.3.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "msw": "^2.11.2",
    "prisma": "^6.16.1",
    "tsx": "^4.20.5",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^5.4.1",
    "zod": "^4.1.8"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
